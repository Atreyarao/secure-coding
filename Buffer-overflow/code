#include<iostream>
#include<string.h>
using namespace std;
int main(){
	char username[10];
	char password[10];
	bool auth=false;
	cout<<"Enter your username"<<endl;
	cin>>username;
	cout<<"Enter your password"<<endl;
	cin>>password;
	if(strcmp(username,"atreya")==0 && strcmp(password,"abcd")==0)
	auth =true;
	if(auth)
	cout<<"Access granted";
	else
	cout<<"wrong username or password";
	return 0;
}
